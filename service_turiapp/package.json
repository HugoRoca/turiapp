{
  "name": "turiapp-service",
  "version": "1.0.0",
  "description": "API service for TuriApp using Koa and MySQL",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.js",
    "build": "eslint src/**/*.js && babel src -d dist --source-maps",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "keywords": ["koa", "mysql", "api", "nodejs"],
  "author": "TuriApp Team",
  "license": "MIT",
  "dependencies": {
    "koa": "^2.14.2",
    "koa-router": "^12.0.0",
    "koa-bodyparser": "^4.4.1",
    "koa-cors": "^0.0.16",
    "koa-logger": "^3.2.1",
    "koa2-swagger-ui": "^5.10.3",
    "swagger-jsdoc": "^6.2.8",
    "mysql2": "^3.6.5",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "koa-jwt": "^4.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
